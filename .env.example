# Aura.build Credentials
AURA_EMAIL=your-email@example.com
AURA_PASSWORD=your-password

# API Keys (for different automation backends)
OAGI_API_KEY=your-lux-api-key
OPENAI_API_KEY=your-openai-key
ANTHROPIC_API_KEY=your-anthropic-key
BROWSER_USE_API_KEY=your-browser-use-key

# Steel.dev (Self-hosted)
STEEL_BASE_URL=http://localhost:3000
# Or Railway deployed: https://your-steel.railway.app

# Proxy Configuration (for API interception)
PROXY_HOST=127.0.0.1
PROXY_PORT=8080

# Agent Configuration
PREFERRED_BACKEND=auto  # auto | api | lux | browser-use | steel
MAX_RETRIES=3
TIMEOUT_MS=60000
HEADLESS=true
DEBUG=false
